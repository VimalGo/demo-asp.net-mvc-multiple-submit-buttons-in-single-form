
@{
    ViewData["Title"] = "Multiple Submit Buttons With FormAction And FormMethod HTML5 Attributes";
}

<h1>Multiple Submit Buttons With FormAction And FormMethod HTML5 Attributes</h1>


@using (Html.BeginForm())
{
    <br />
    <input type="submit" name="save" value="Save" formaction="SaveForm" formmethod="post" />
    <input type="submit" name="cancel" value="Cancel" formaction="CancelForm" formmethod="post" />
    <br />
    <br />
    @ViewBag.Message
}